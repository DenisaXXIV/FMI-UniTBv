<div class="nb-cell program" name="p1">
member(X, [X|_]).
member(X, [_|T]) :- 
    member(X, T).

delete_repeats([], _, []).  
delete_repeats([H|T], Seen, [H|R]) :- 
    \+ member(H, Seen),
    delete_repeats(T, [H|Seen], R).
delete_repeats([H|T], Seen, R) :-  
    member(H, Seen),
    delete_repeats(T, Seen, R).

main(L) :-
    delete_repeats(L,[],R),
    writeln(R).
